	spawn
	{
		accum 0 bitreset 0
		accum 1 set 0
		trigger radar rotateloop 
	}

	trigger rotateloop
	{
		faceangles 0 90 0 1000
		accum 1 set 2
		trigger radar choosefall
		faceangles 0 180 0 1000
		accum 1 set 3
		trigger radar choosefall
		faceangles 0 270 0 1000
		accum 1 set 4
		trigger radar choosefall
		faceangles 0 360 0 1000
		accum 1 set 1
		trigger radar choosefall
		trigger radar rotateloop
