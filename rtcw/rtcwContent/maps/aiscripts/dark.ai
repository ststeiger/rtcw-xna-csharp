nazi1				
{
	attributes
	{
		starting_health 40
		aim_accuracy 0.3
		tactical 1.0
		aggression 1.0
		inner_detection_radius 200
		alertness 2300		
	}

	spawn
	{
		accum 0 bitreset 0
		accum 1 set 0
		accum 2 set 0
		accum 3 set 0
		statetype alert
		trigger nazi1 go1	
	}

	trigger go1
	{
		runtomarker n1spot1
		wait 500 nazi1a
		trigger nazi1 marchloop1
	}

	trigger marchloop1
	{
		walktomarker n1spot1
		wait 3000
		walktomarker n1spot2
		facetargetangles n1spot2
		wait 4000
		walktomarker n1spot3
		facetargetangles n1spot3
		wait 4000
		trigger nazi1 marchloop1
	}
	
	trigger go1			
	{
	}

	pain 10
	{
		attrib aggression 1.0
		attrib camper 1.0
		giveweapon weapon_mp40
		setammo ammo_9mm 1000
		selectweapon weapon_mp40
		playsound MiscKillHim
	}

	enemysight
	{
		accum 0 bitset 20
		trigger checker2 sight_checkin
		playsound MiscAmerican
		clearanim
		trigger nazi1 choose_tactic
	}
	
	trigger choose_tactic
	{
		accum 2 random 3
		wait 5
		trigger nazi1 tactic1
		wait 5
		trigger nazi1 tactic2
		wait 5
		trigger nazi1 tactic3
	}

	trigger tactic1			// total camper
	{
		accum 2 abort_if_not_equal 0
		resetscript
	//	print tactic1
		attrib camper 1.0
		attrib aggression 0.1
		attrib aim_accuracy 0.5
		attrib reaction_time 0.9
		runtomarker n1attack1 
		wait 3000 player
	}

	trigger tactic2			// Super aggressive
	{
		accum 2 abort_if_not_equal 1
		resetscript
	//	print tactic2
		attrib camper 0.1
		attrib aggression 1.0
		attrib aim_accuracy 0.3
		runtomarker n1attack2 
	}

	trigger tactic3			// medium
	{
		accum 2 abort_if_not_equal 2
		resetscript
	//	print tactic3
		attrib reaction_time 0.9
		attrib camper 0.6
		attrib aggression 0.7
		attrib aim_accuracy 0.1
		runtomarker n1attack3
		wait 4000 player
	}

	inspectsoundstart
	{
		clearanim
	}

	inspectsoundend
	{
	}

	inspectbodystart
	{
		clearanim
	}

	inspectbodyend
	{
	}	

	death
	{
		trigger checker1 death_check_out
//		trigger checker1 backup_for_zone2
		accum 0 abort_if_not_bitset 20
		trigger checker2 sight_check_out
	}
}

