nazi6
{
	attributes
	{
		starting_health 35
		hearing_scale 1.0
		crouching_speed 150
		idle_crouch 0.0
		walking_speed 90
		running_speed 275
		alertness 1800
	}

	spawn
	{
		accum 0 set 1	// AI Position (0=Disabled, 1=Way Before Tunnel, 2=Before Tunnel, 3=In Tunnel, 4=End of Tunnel)
		accum 1 set 0	// AI Position Past Tunnel while proceeding (0=Disabled, 1=Lower Bunker, 2 Upper Bunker)
		accum 2 set 1	// Checks to see if an event has already triggered (0=Disabled, 1=Enabled)
		setammo ammo_grenades 3
		statetype alert
		trigger nazi6 go
	}

	trigger go	// This tells the AI to run towards the MG42, through the tunnel.
	{
		wait 1000
		noavoid
		runtomarker n6spot1 nostop
		runtomarker n6spot2 nostop
		runtomarker n6spot3 nostop
		runtomarker n6spot4 nostop
		accum 0 set 2
		trigger nazi7 set0to2
		trigger nazi8 set0to2
		trigger nazi9 set0to2
		runtomarker n6spot5 nostop
		runtomarker n6spot6 nostop
		runtomarker n6spot7 nostop
		accum 0 set 3
		trigger nazi7 set0to3
		trigger nazi8 set0to3
		trigger nazi9 set0to3
		runtomarker n6spot8 nostop
		runtomarker n6spot9
		accum 0 set 4
		trigger nazi7 set0to4
		trigger nazi8 set0to4
		trigger nazi9 set0to4
		wait 5
		trigger nazi7 no_follow
		wait 1000
		walktomarker n6spot9a
		wait 500 nazi12
		trigger nazi13 gomarch1	// TELLS THE OTHER AI TO BEGIN THEIR MOVEOUT SCRIPT
		trigger nazi12 moveout
		trigger nazi11 moveout
		trigger nazi10 moveout
		trigger nazi9 gomarch1
		trigger nazi8 moveout
		trigger nazi7 moveout
		trigger nazi6 moveout
	}

	// Script to check out the bunker. Nazi10 will join Nazi6
	trigger moveout
	{
		wait 1000
		attrib running_speed 220
		avoid
		runtomarker n1spot3 nostop
		runtomarker n1spot4 nostop
		runtomarker n1spot5 nostop
		runtomarker n6spot10
		accum 1 set 2
		wait 2000
		wait 1500
		trigger nazi10 man_mg42
		trigger nazi6 lower_bunker
	}

	// Script to check out the lower portion of the bunker.
	trigger lower_bunker
	{
		runtomarker n1spot5 nostop
		runtomarker n1spot4 nostop
		runtomarker n1spot3 nostop
		runtomarker n4spot1 nostop
		wait 500
		crouchtomarker n6spot11 nostop
		accum 1 set 1
		crouchtomarker n6spot12
		crouchtomarker n5spot1 nostop
		runtomarker n6spot13 nostop
		runtomarker n6spot14
		runtomarker n6spot10 nostop
		runtomarker n1spot5
		accum 1 set 2
		wait 500
		wait 1500
		walktomarker n6spot15
		facetargetangles n6spot15
		wait forever 0		
	}

	// This will choose the correct "EVADE" script.
	trigger choose_evade_tunnel
	{
		accum 0 abort_if_equal 0
		resetscript
		trigger nazi6 evade_way_before_tunnel
		wait 5
		trigger nazi6 evade_before_tunnel
		wait 5
		trigger nazi6 evade_in_tunnel
		wait 5
		trigger nazi6 evade_after_tunnel
	}

	// If AI is not even near the tunnel and sees the player, this will trigger.
	trigger evade_way_before_tunnel
	{
		accum 0 abort_if_not_equal 1
		accum 0 set 0
		resetscript
		trigger nazi7 cover_way_before_tunnel
		trigger nazi8 cover_way_before_tunnel
		trigger nazi9 cover_way_before_tunnel
		trigger nazi10 cover_way_before_tunnel
		trigger nazi11 cover_way_before_tunnel
		trigger nazi12 cover_way_before_tunnel
		trigger nazi13 cover_way_before_tunnel
		trigger nazi6 cover_way_before_tunnel
