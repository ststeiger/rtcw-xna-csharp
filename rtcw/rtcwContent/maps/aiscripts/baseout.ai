nazi1
{
	attributes
	{
		starting_health 35
		alertness 1280
		fov 240
		walking_speed 90
		running_speed 220
	}

	spawn
	{
		accum 0 set 0
		statetype alert
		wait forever 0
	}

	trigger go
	{
		runtomarker n1spot1
		wait 1500
		trigger mech1 talk1
	}

	trigger talk2
	{
		wait 1500
		trigger mech1 talk2
	}

	trigger talk3
	{
		wait 500 mech1
		trigger mech1 talk3
	}

	trigger talk4
	{
		runtomarker n1spot2 nostop
		attrib fov 240
		runtomarker n1spot3
		trigger nazi1 man_m42
	}

	trigger man_mg42		// Nazi1 goes back to man the MG42.
	{
		runtomarker n1spot2 nostop
		accum 0 set 1		// This will tell Nazi2 not go to the MG42 as nazi2 is now closer to it.
		runtomarker n1spot3 nostop
		runtomarker n1spot4 nostop
		runtomarker n1spot5 nostop
		runtomarker mg42_1_spot1
		mount mg42_1
		attrib aim_accuracy 0.25
		attrib attack_skill 0.3
		attrib aim_skill 0.2
		accum 0 set 3
	}

	trigger choose_mg42
	{
		trigger nazi1 man_mg42_combat
		wait 5
		trigger nazi1 go_combat
	}

	trigger man_mg42_combat
	{
		accum 0 abort_if_not_equal 1
		wait 500 0 nazi2
		trigger nazi2 go_combat
		runtomarker n1spot3 nostop
		runtomarker n1spot4 nostop
		runtomarker n1spot5 nostop
		runtomarker mg42_1_spot1
		mount mg42_1
		attrib aim_accuracy 0.25
		attrib attack_skill 0.3
		attrib aim_skill 0.2
	}

	trigger go_combat
	{
		accum 0 abort_if_not_equal 0
		accum 0 set 1
		trigger nazi2 man_mg42
		wait 500
		attrib camper 0.8
		resetscript
		attack
	}

	inspectfriendlycombatstart
	{
		deny
	}

	bulletimpact
	{
		deny
	}

	inspectsoundstart
	{
		deny
	}

	inspectbodystart
	{
		deny
	}

	death
	{
		accum 0 set 1	
		trigger nazi2 man_mg42
	}

	enemysight
	{
		resetscript
		accum 0 abort_if_equal 3	// If nazi1 is already on the MG42, do not continue.
		trigger nazi1 choose_mg42
	}

}

