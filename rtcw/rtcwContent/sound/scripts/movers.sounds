// sound scripts for movers (doors/buttons)
//
// channels: local, announcer, body, voice, weapon, item, auto (default)
// toggles: streaming, looping (only works for streaming sounds), global


///////////////// Doors

//
// door type 1
//

// regular open/close

door1_open {
	sound sound/movers/doors/door5_open.wav
}

door1_endo {
	sound sound/movers/doors/door5_endo.wav
}

door1_close {
	sound sound/movers/doors/door5_close.wav
}

door1_endc {
	sound sound/movers/doors/door5_endc.wav
}

door1_loopo {
	sound sound/movers/doors/door5_loopo.wav
}

door1_loopc {
	sound sound/movers/doors/door5_loopc.wav
}

door1_locked {
	sound sound/movers/doors/door5_locked.wav	// referenced in other areas
}

// quiet open/close

door1_openq {
	sound sound/movers/doors/door5_openq.wav
}

door1_endoq {
	sound sound/movers/doors/door5_endoq.wav
}

door1_closeq {
	sound sound/movers/doors/door5_closeq.wav
}

door1_endcq {
	sound sound/movers/doors/door5_endcq.wav
}

// kicked open

door1_kicked {
	sound sound/movers/doors/door5_kicked.wav
}

door1_kickedend {
	sound sound/movers/doors/door5_locked.wav		// shared
}

